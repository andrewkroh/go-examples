{
    "mappings": {
        "dynamic_templates": [
            {
                "strings_as_keyword": {
                    "mapping": {
                        "ignore_above": 1024,
                        "type": "keyword"
                    },
                    "match_mapping_type": "string"
                }
            }
        ],
        "properties": {
            "default": {
                "type": "keyword"
            },
            "elasticsearch": {
                "properties": {
                    "index_template": {
                        "properties": {
                            "mappings": {
                                "properties": {
                                    "dynamic": {
                                        "type": "keyword"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "example": {
                "type": "keyword"
            },
            "icons": {
                "type": "nested"
            },
            "inputs": {
                "type": "nested"
            },
            "name": {
                "fields": {
                    "text": {
                        "analyzer": "variable_name",
                        "type": "text"
                    }
                },
                "type": "keyword"
            },
            "sample_event": {
                "type": "flattened"
            },
            "screenshots": {
                "type": "nested"
            },
            "streams": {
                "type": "nested"
            }
        }
    },
    "settings": {
        "analysis": {
            "analyzer": {
                "variable_name": {
                    "tokenizer": "variable_name_tokenizer"
                }
            },
            "tokenizer": {
                "variable_name_tokenizer": {
                    "pattern": "[-_.]",
                    "type": "simple_pattern_split"
                }
            }
        },
        "index": {
            "mapping": {
                "total_fields": {
                    "limit": "10000"
                }
            }
        },
        "number_of_replicas": 0,
        "number_of_shards": 1
    }
}
